<% provide(:title, 'Feedback') %>

<h1>Listing feedbacks</h1>

<table>
  <tr>
    <th>Headline</th>
    <th>Rating</th>
    <th>User</th>
    <th>Project</th>
    <th>Scene</th>
    <th>Character</th>
    <th>Location</th>
    <th>Music</th>
    <th>Inspiration</th>
    <th>Comments</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feedbacks.each do |feedback| %>
  <tr>
    <td><%= feedback.headline %></td>
    <td><%= feedback.rating %></td>
    <td><%= feedback.user_id %></td>
    <td><%= feedback.project_id %></td>
    <td><%= feedback.scene_id %></td>
    <td><%= feedback.character_id %></td>
    <td><%= feedback.location_id %></td>
    <td><%= feedback.music_id %></td>
    <td><%= feedback.inspiration_id %></td>
    <td><%= feedback.comments %></td>
    <td><%= link_to 'Show', feedback %></td>
    <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
    <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feedback', new_feedback_path %>
